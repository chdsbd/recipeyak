    "http-proxy-middleware": "0.17.3",
    "jest": "^22.0.4",
    "jest-enzyme": "^4.0.1",
    "json-loader": "0.5.4",
    "node-sass": "^4.7.2",
    "object-assign": "4.1.1",
    "postcss-loader": "^2.0.9",
