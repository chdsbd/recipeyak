    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.7.1",
    "url-loader": "0.5.7",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.11.1",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bulma": "^0.4.2",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "postcss-flexbugs-fixes": "^3.2.0",
    "query-string": "^5.0.1",
    "raven-js": "^3.20.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-textarea-autosize": "^5.2.1",
    "redux": "^3.7.1",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "dev": "node scripts/start.js",
    "build": "node scripts/build.js",
    "lint": "npm run jslint && npm run tslint",
    "lint-fix": "npm run jslint-fix && npm run tslint-fix",
    "jslint": "node scripts/lint.js",
    "jslint-fix": "node scripts/lint.js --fix",
    "tslint": "tslint 'src/**/*.{ts,tsx}' --format 'codeFrame'",
    "tslint-fix": "tslint 'src/**/*.{ts,tsx}' --format 'codeFrame' --fix",
    "test": "node scripts/test.js --env=jsdom",
    "test-cov": "node scripts/test.js --env=jsdom --coverage --runInBand"
  },
  "proxy": "http://django:8000",
  "jest": {
    "setupTestFrameworkScriptFile": "jest-enzyme",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "raf/polyfill"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app",
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-do-expressions",
      "transform-class-properties"
    ]
  }
}
